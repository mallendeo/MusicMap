{
  "manifest_version": 2,
  "name": "Youtify",
  "version": "0.0.1",
  "description": "Open the current song in Spotify",
  "author": "mallendeo",
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/**",
        "*://open.spotify.com/**"
      ],
      "js": ["inject.js"]
    }
  ],
  "permissions": [
    "*://*.youtube.com/**",
    "*://open.spotify.com/**"
  ],
  "web_accessible_resources": [
    "youtify.js",
    "youtify.css"
  ]
}
